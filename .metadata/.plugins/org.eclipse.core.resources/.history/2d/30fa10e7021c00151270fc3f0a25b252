package com.automatic.test;


import org.testng.Assert;
import org.testng.annotations.Test;

import com.automatic.pages.ILogLevel;
import com.automatic.pages.TestCore;
import com.automatic.projectPages.LinkedInPage;

public class LinkedInTest extends TestCore{
	
	@Test(priority=0)
	public void testLinkedIn(){
		LinkedInPage lnkdin = new LinkedInPage(driver);
		log("Testing [LinkedIn] Page", ILogLevel.TEST);
		lnkdin.signinLinkedIn();
		
		log("Testing [LinkedIn] Page", ILogLevel.TEST);
		lnkdin.SignOutLinkedIn();
		
		log("Text on [You have signed out is:] Verify", ILogLevel.ASSERTS);
		Assert.assertEquals(lnkdin.verifySignout(), "You have signed out");
		
	}

	@Test(priority=1)
	public void testShowGroups(){
		
		LinkedInPage lnkdin = new LinkedInPage(driver);
		log("Login with linkedin", ILogLevel.TEST);
		lnkdin.signinLinkedIn();
		
		lnkdin.ShowGroups();
	}
	
	
	
}
